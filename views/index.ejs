<%- include('./partials/head'); -%>
<%- include('./partials/header'); -%>




<section class="article_post">
	<ul>
		<% posts.forEach(post => { %>
			<li>
				<a href="/posts/<%= post.id %>">
				<h3><%- post.title.rendered %></h3>
				<p><%= post.featured_media %></p>
				<p><%- post.date %></p>
				<p><%- post.author %></p>
				<caption><%= post.caption %></caption>
				
				<img decoding="async" src="<%= post.yoast_head_json.og_image[0].url %>" alt="" width="<%= post.yoast_head_json.og_image[0].width %>">
 				</a>
			</li>
		<% }); %>	
	</ul>
</section>



<%- include('./partials/foot'); -%>